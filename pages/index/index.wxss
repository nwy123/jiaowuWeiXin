/**index.wxss**/
@import "../classroom/pages/main/main.wxss";
page{
  height: 100%;
}
/*main.wxss更改增加*/
.kind-list-item{
  margin-bottom:20rpx;
}
.navigator-text{
  color:#878b8a;
  font-size:28rpx;
}
.name_box{
  margin-bottom: 20rpx;
}
.main_head_text{
  background:rgba(0,0,0,0.5)
}
.main_box{
  width: 93%;
  margin-left:3.5%;
}
.cell-box{
  display: flex;
  flex-direction: column;
 
  align-items: center;
  width:100%;
  height:100%;
}
.course-cell{
  position: relative;
  width:700rpx;
  height:150rpx;
  background: #fff;
  margin:20rpx;
  margin-top: 0;
  margin-bottom: 0;
  display: flex;
  align-items: center;
}
.course-cell-before{
  content: " ";
  position: absolute;
  left: 30rpx;
  top: 0;
  right: 30rpx;
  height: 1px;
  border-top: 0.5rpx solid #f5f5f5;
  color: #e6e6e6;
}

.cell-icon{
  width:60rpx;
  height:60rpx;
  border-radius: 50%;
  background:#00aff0;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-left:50rpx;
}
.cell-icon image{
  width:50rpx;
  height:50rpx;
}

.cell-text{
  width:480rpx;
  display: flex;
  flex-direction: column;
 justify-content: center;
  margin-left:37rpx;

}
.course-name{
  font-size:14px;
  color:black;
}

.class{
  display: flex;
  justify-content: flex-start;
  margin-top:20rpx;

}
.mar10{
  margin-left: 10rpx;
}
.class-name{
  font-size:12px;
  color: #ccc;
}

.cell-select{
  width:120rpx;
  height:60rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 10rpx;
  background:#00aff0;
  font-size:14px;
  color:white;
}