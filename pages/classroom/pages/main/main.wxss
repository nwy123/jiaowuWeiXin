
page{
  align-items: center;
  justify-content: center;
  background-color: #ebe7e8;
  height: 100%;
  font-size: 32rpx;
  line-height: 1.6;
  height: 100%;
}
.main_box{
  width: 95%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  margin-left:2.5%;
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
  overflow: hidden;
  margin-top:20rpx;
}
.main_head_box{
  width:100%;
  height:300rpx;
  margin-top:20rpx;
  position: relative;
  border-radius: 10rpx;
  border-bottom-left-radius: 0;
  border-bottom-right-radius: 0;
  overflow: hidden;
}
.main_box_img{
  width: 100%;
  height:100%;
}
.main_head_text{
  padding-left:40rpx;
  width:100%;
  height:105rpx;
  position: absolute;
  left:0;
  bottom:0;
  background: rgba(0,0,0,0.2);
  display: flex;
  flex-direction: column;
  justify-content: center;
  font-size: 32rpx;
  color: #fff;
  z-index: 1;
}

.navigator-box {
  opacity: 0;
  position: relative;
  background-color: #FFFFFF;
  line-height: 1.41176471;
  font-size: 34rpx;
  transform: translateY(-50%);
  transition: .3s;
}
.navigator-box-show {
  opacity: 1;
  transform: translateY(0);
}
.navigator {
  padding: 30rpx 30rpx;
  position: relative;
  display: flex;
  align-items: center;
}
.navigator:before {
  content: " ";
  position: absolute;
  left: 30rpx;
  top: 0;
  right: 30rpx;
  height: 1px;
  border-top: 0.5rpx dashed #ededed;
  color: #5c5c5c;
}
.navigator-text {
  flex: 1;
  color:#696969;
  margin-left: 20rpx;
  font-size: 34rpx;
}
.navigator-arrow {
  padding-right: 26rpx;
  position: relative;

}
.navigator-arrow:after {
  content: " ";
  display: inline-block;
  height: 18rpx;
  width: 18rpx;
  border-width: 2rpx 2rpx 0 0;
  border-color:#b3b3b3;
  border-style: solid;
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  position: absolute;
  top: 50%;
  margin-top: -8rpx;
  right: 28rpx;
}

.kind_list{
  margin-bottom: 20rpx;
}
.kind-list-item {
  background-color: #FFFFFF;
  border-radius: 10rpx;
  overflow: hidden;
  position: relative;
  margin-top: 40rpx;
  box-shadow: 2rpx 2rpx 2rpx 2rpx #ccc inset;

  
}
.kind-list-item:before {
  content: " ";
  position: absolute;
  left: 30rpx;
  top: 0;
  right: 30rpx;
  height: 1px;
  border-top: 0.5rpx solid #f5f5f5;
  color: #e6e6e6;
}
.kind-list-item:first-child {
  margin-top: 40rpx;
}
.kind-list-text{
  flex: 1;
}
.kind-list-img {
  width: 60rpx;
  height: 60rpx;
}
.kind-list-item-hd {
  padding: 24rpx;
  background: #fff;
  display: flex;
  align-items: center;
  padding-left: 30rpx;
  padding-right: 30rpx;
  color: #242817;
  font-size: 28rpx;

  transition: opacity .3s;
}
.kind-list-item-hd-show {
  opacity: .2;
}
.kind-list-item-bd {
  height: 0;
  overflow: hidden;
}
.kind-list-item-bd-show {
  font-size: 32rpx;
  height: auto;
}

.weui-agree__link {
  display: inline;
  color: #586c94;
}

.navigator_img{
  width: 60rpx;
  height: 60rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.navigator_img image{
  width: 50rpx;
  height: 50rpx;

}

