/* pages/selectWay/selectWay.wxss */
page{
  height:100%;
}
.select-box{
	width: 610rpx;
	height: auto;
	background: #fff;
	border-radius: 10rpx;
	margin-top: 30rpx;
	padding:40rpx;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;

}

.box-add{
  margin-top:90rpx;
  padding-bottom:100rpx;
}
.select-text{
	font-size: 32rpx;
  margin-top:20rpx;
}

.select-ball{
	display: flex;
	width: 100%;
	justify-content: space-around;
	margin-top: 40rpx;
}
.select-cell{
	height: 315rpx;
	width: 250rpx;
	border: solid #ededed 1rpx;
	border-radius: 10rpx;
	display: flex;
	flex-direction: column;
	justify-content: center;
	align-items: center;
	padding-bottom: 10rpx;
	

}
.select-icon{
	display: flex;
	width: 100rpx;
	height: 100rpx;
	margin-top: 40rpx;
}
.select-dcu{
	
	flex-direction: column;
	justify-content: center;
	align-items: center;
}
.duc1{
	
	font-size: 28rpx;
	margin-top: 50rpx;
}
.duc2{
	display: flex;
	justify-content: center;
	align-items: center;
	text-align: center;
	font-size: 24rpx;
	color: #ccc;
	margin-top: 30rpx;
}

.jl-nav{
	width: 100%;
	display: flex;
	justify-content: flex-end;
}
.jl-btn{
  width:160rpx;
  height: 50rpx;
  background:#44baff;
  color:white;
  font-size:24rpx;
  border-radius: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-right: 0;
}